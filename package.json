{
  "name": "guide-ide",
  "version": "1.6.9",
  "description": "guIDE - AI-Powered Offline IDE with local LLM, RAG, MCP tools, browser automation, and integrated terminal",
  "author": {
    "name": "Brendan Gray",
    "url": "https://github.com/FileShot"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/FileShot/guIDE"
  },
  "main": "electron-main.js",
  "homepage": "./",
  "scripts": {
    "dev": "node scripts/launch.js",
    "dev:renderer": "vite --port 5174 --strictPort",
    "dev:main": "cross-env NODE_ENV=development npx electron .",
    "build": "npm run build:renderer && npm run build:main",
    "build:nosign": "node scripts/bump-version.js && npm run build:renderer && electron-builder --config electron-builder.nosign.json && node scripts/deploy-update.js",
    "build:nosign:cuda": "electron-builder --config electron-builder.nosign.cuda.json",
    "build:all": "node scripts/bump-version.js && npm run build:renderer && electron-builder --config electron-builder.nosign.json && electron-builder --config electron-builder.nosign.cuda.json && node scripts/deploy-update.js",
    "postbuild": "node scripts/deploy-update.js",
    "build:renderer": "vite build",
    "build:main": "electron-builder",
    "preview": "vite preview",
    "postinstall": "electron-rebuild -f -w node-pty || echo 'node-pty rebuild skipped'",
    "type-check": "tsc --noEmit",
    "release:bump": "node scripts/bump-version.js",
    "release:deploy": "node scripts/deploy-update.js"
  },
  "dependencies": {
    "@xterm/addon-fit": "^0.9.0",
    "@xterm/addon-search": "^0.14.0",
    "@xterm/addon-web-links": "^0.10.0",
    "@xterm/xterm": "^5.4.0",
    "chokidar": "^3.5.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "electron-updater": "^6.7.3",
    "esbuild": "^0.27.3",
    "file-icon-vectors": "^1.0.0",
    "fuse.js": "^7.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.294.0",
    "marked": "^11.0.0",
    "mermaid": "^11.12.2",
    "monaco-editor": "^0.44.0",
    "node-llama-cpp": "^3.17.1",
    "node-pty": "^1.0.0",
    "playwright-core": "^1.58.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^4.18.1",
    "sql.js": "^1.14.0",
    "tailwind-merge": "^2.0.0",
    "uuid": "^9.0.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^34.0.0",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.guide.ide",
    "productName": "guIDE",
    "copyright": "Copyright Â© 2025-2026 Brendan Gray",
    "directories": {
      "output": "dist-electron"
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "preload.js",
      "main/**/*",
      "scripts/**/*",
      "node_modules/**/*",
      "!node_modules/.cache",
      "!node_modules/**/test/**",
      "!node_modules/**/tests/**",
      "!node_modules/**/*.md",
      "!node_modules/**/CHANGELOG*",
      "!node_modules/**/LICENSE*",
      "!node_modules/@node-llama-cpp/win-x64-cuda/**",
      "!node_modules/@node-llama-cpp/win-x64-cuda-ext/**",
      "LICENSE"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "icon.ico",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "artifactName": "guIDE-${version}-${arch}.${ext}"
    },
    "win": {
      "icon": "icon.ico",
      "certificateFile": "code-signing.pfx",
      "certificatePassword": "guIDE2026!",
      "publisherName": "GraySoft LLC",
      "signingHashAlgorithms": [
        "sha256"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "guIDE",
      "installerIcon": "icon.ico",
      "uninstallerIcon": "icon.ico",
      "include": "build/installer.nsh",
      "installerSidebar": "build/installerSidebar.bmp",
      "license": "LICENSE",
      "displayLanguageSelector": false,
      "runAfterFinish": true,
      "artifactName": "guIDE-Setup-${version}.${ext}"
    },
    "linux": {
      "icon": "icon.ico",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development",
      "artifactName": "guIDE-${version}-${arch}.${ext}"
    },
    "asar": true,
    "compression": "maximum",
    "publish": {
      "provider": "generic",
      "url": "https://graysoft.dev/updates"
    },
    "asarUnpack": [
      "node_modules/node-llama-cpp/**",
      "node_modules/@node-llama-cpp/**",
      "node_modules/@huggingface/**",
      "node_modules/@octokit/**",
      "node_modules/@kwsites/**",
      "node_modules/octokit/**",
      "node_modules/lifecycle-utils/**",
      "node_modules/ipull/**",
      "node_modules/env-var/**",
      "node_modules/chmodrp/**",
      "node_modules/filenamify/**",
      "node_modules/pretty-ms/**",
      "node_modules/parse-ms/**",
      "node_modules/stdout-update/**",
      "node_modules/async-retry/**",
      "node_modules/bytes/**",
      "node_modules/proper-lockfile/**",
      "node_modules/validate-npm-package-name/**",
      "node_modules/chalk/**",
      "node_modules/cmake-js/**",
      "node_modules/cross-spawn/**",
      "node_modules/debug/**",
      "node_modules/fs-extra/**",
      "node_modules/ignore/**",
